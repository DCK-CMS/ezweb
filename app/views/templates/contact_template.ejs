<% include ./partials/navBar %>

<!-- Main content -->
<div class="main text-center">
  <div class="contact-wrapper">

    <div class="contact-header">
      <h3><%= pageData.header %></h3>
    </div>

    <div class="logo-wrapper">
      <!-- Logo -->
      <%if(url.length > 0){%>
        <img class="contact-logo" src="<%=url[0].value%>" />
      <%}else{%>
        <img class="contact-logo" src="http://i.imgur.com/351vaj3.png" />
      <%}%>
    </div>

    <div class="contact-details">
      <h4>Location: <span><%= pageData.address %></span></h4>
      <h4>Contact no: <span><%= pageData.contact_no %></span></h4>
    </div>

  </div> <!-- *end of contact wrapper -->

</div>
