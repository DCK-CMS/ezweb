<% include ./partials/navBar %>

<!-- Carousel -->
<% include ./partials/carousel %>

<!-- Main content -->
<div class="main">
  <div class="row">
    <div class="col-md-6 about-section-col">
      <!-- Title -->
      <div class="about-section-header">
        <h3><%= pageData.title %></h3>
      </div>
      <!-- Description -->
      <div class="about-section-desc">
        <p> <%= pageData.text %> </p>
      </div>
    </div>

    <%if(pageData.img.length > 0 ){%>
    <div class="col-md-5 about-img-col">
      <img src="<%= pageData.img[0].img_url %>" alt="" />
    </div>
    <%}%>
  </div>
</div>
